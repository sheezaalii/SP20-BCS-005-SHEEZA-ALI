
<table class="table table-hover">
    <thead>
        <tr>
            <th>Product Name</th>
            <th>Product Price</th>
        </tr>

    </thead>
    <tbody>
        <% for(var i=0;i<products.length;i++) {let product = products[i];%>
            <tr>
                <td><%=product.name  %> </td>
                <td><%=product.price  %> </td>
            </tr>
            <% } %> 
            <tfoot>
                <tr>
                    <td>Total:</td>
                    <td><%=total  %> </td>
                </tr>
            </tfoot>
    </tbody>
</table>


<a href="/product" class="my-button my-font-2">Back to Bakies</a>

